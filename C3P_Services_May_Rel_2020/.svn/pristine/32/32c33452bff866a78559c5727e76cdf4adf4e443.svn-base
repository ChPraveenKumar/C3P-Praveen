<div class="container main-pg">

  <div class="row">

    <!--    Page Header Section     -->
    <app-header></app-header>

    <div class="pg-bdy">
      <!-- Breadcrumb Section -->
      <ul class="breadcrumb">
        <li>
          <a routerLink="/home">C3P</a>
        </li>
        <li class="breadcrumbLabel">Dashboard</li>
        <li class="active">Config Comparison</li>
      </ul>
      <div class="db-search-section">
        <div class="row">
          <div class="col-lg-6">
            <div class="form-inline">
              <div class="form-group">
                <label class="mr-5">Search By : </label>
                <select class="form-control" [(ngModel)]="searchField" (change)="clearSearch()">
                  <option *ngFor="let item of searchItems" value="{{item}}">{{item}}</option>
                </select>
                <!-- <input type="text" class="form-control" (keyup)="searchConfigRecord(searchInput)" placeholder="Enter search text" name="searchInput"
                  [(ngModel)]="searchInput" /> -->
                <input type="text" class="form-control" placeholder="Enter search text" [(ngModel)]="searchInput" />
                <button type="submit" class="btn ico-search" (click)="configCompareSearchRequest(searchField,searchInput)"></button>
                <span class="col-md-4" style="color: red;font-size: 14px;margin-left:85%;margin-top: -5%" *ngIf="errormessage">Record not found</span>
              </div>
            </div>
          </div>
        </div>
      </div>

      <!-- Summary View PopUp -->
      <div id="summaryPopUp" class="reportPopUp">
        <div id="summaryPopUpContainer" ngDraggable class="popUpContainerSummary" style="overflow:hidden !important;">
          <!-- Modal content -->
          <div class="popup-content">
            <div id="summaryPopUpContainerHeader" class="col-lg-12 popup-content-header" style="margin-left:0%;">
              <span class="small_logo_icon"></span>
              <label class="report-header-style">
                <h4 class="fs-16">Config Comparison Summary View</h4>
              </label>
              <span class="report-close-icon" (click)="closesummaryPopUp('summaryPopUp')"></span>
            </div>
            <div class="reportPopUpContainer col-lg-12" style="overflow:hidden !important;">
               
              <div id="summaryData">
                <!-- <div *ngIf="loader" img src="../imgs/spinner.gif">

                </div> -->
              </div>

              <span id="loadText"></span>
            </div>
          </div>
        </div>
      </div>

      <!-- Detailed View PopUp -->
      <div id="detailsPopUp" class="reportPopUp">
        <div id="detailsPopUpContainer" ngDraggable class="popUpContainerDetails" style="overflow:hidden !important;">
          <!-- Modal content -->
          <div class="popup-content">
            <div id="detailsPopUpContainerHeader" class="col-lg-12 popup-content-header">
              <span class="small_logo_icon"></span>
              <label class="report-header-style">
                <h4 class="fs-16">Config Comparison Detailed View</h4>
              </label>
              <span class="report-close-icon" (click)="closedetailsPopUp('detailsPopUp')"></span>
            </div>
            <div class="reportPopUpContainer col-lg-12" style="overflow:hidden !important;">
              <div id="accordion">
               
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="db-result-section">
        <div (scroll)="tableScroll()" class="pane-hScroll table-responsive">
          <table style="margin-bottom: 0px;" st-table="requestsList" class="table table-striped table-hover hdr-table hdr-sort" st-safe-src="rowCollection">
            <thead>
              <tr>
                <th id="requestId" class="sort-ascent" (click)="setOrder('requestId')" style="width: 27%;">Service Request</th>
                <th id="status" class="sort-ascent" (click)="setOrder('status')">Status</th>
                <th id="reqReceivedOn" class="sort-ascent" (click)="setOrder('reqRecvdOn')">Request Received On</th>
                <th id="reqProcessedOn" class="sort-ascent" (click)="setOrder('reqProcessedOn')">Request Processed On</th>
                <th id="comment" class="sort-ascent" (click)="setOrder('comment')">Comment</th>
              </tr>
            </thead>
          </table>
          <div class="pane-vScroll">
            <table st-table="gridDataOutput" class="table table-striped table-hover" st-safe-src="rowCollection">
              <tbody>
                <tr>
                  <td class="text_bold" style="text-align: left;width: 27%;">
                    <span (click)="expandCollapseParent($event)">
                      <span [ngClass]="{'ico-collapse': showMe ,'ico-expand': !showMe}"></span>
                    </span>
                    D9C61BA2-B
                    <br/>
                    <!-- summary -->
                    <div [hidden]="!showMe" class="summary">
                      <div class="text_bold" style="margin-top:2%; width: 30%;margin-left: 2%;;font-weight: 100;color: white;">
                        <span (click)="summaryExpandCollapse($event)">
                          <span [ngClass]="{'ico-collapse-summarychild': showMeChild,'ico-expand-summarychild': !showMeChild}"></span>
                        </span>
                        <span style="color: white;background-color: #4865A9;">Summary</span>
                      </div>
                      <div [hidden]="!showMeChild" class="summaryview" style="line-height:1.2;margin-left:5%;margin-top:2%;">
                        <div>
                          <span style="text-align: center">
                            <span>
                              <a style="cursor: pointer;color:green" (click)="openSummaryPopUp('D9C61BA2-B','FC_LAC','summary')">FC.20.0.0.1-LAC.20.0.0.1</a>
                              <br/>
                              <a style="cursor: pointer;color:green" (click)="openSummaryPopUp('D9C61BA2-B','LBFR_CR','summary')">LBFR.20.0.0.1-CR.20.0.0.1</a>
                              <br/>
                              <a style="cursor: pointer;color:green" (click)="openSummaryPopUp('D9C61BA2-B','LBFR_LASR','summary')">LBFR.20.0.0.1-LASR.20.0.0.1</a>
                              <br/>
                              <a style="cursor: pointer;color:green" (click)="openSummaryPopUp('D9C61BA2-B','LBFR_LAFR','summary')">LBFR.20.0.0.1-LAFR.20.0.0.1</a>
                              <br/>

                            </span>
                            <!-- <span *ngIf="configSummary.value == 0">
                              <span style="color:black">{{configSummary.key}}</span>
                            </span> -->
                          </span>
                        </div>
                      </div>
                    </div>

                    <!-- detailed Summary -->
                    <div [hidden]="!showMe" class="details">
                      <div class="text_bold" style="margin-top:2%;; width: 45%;font-weight: 100;margin-left: 2%;color: white;">
                        <span (click)="detailsExpandCollapse($event)">
                          <span [ngClass]="{'ico-collapse-detailchild': showMeDetailChild,'ico-expand-detailchild': !showMeDetailChild}"></span>
                        </span>
                        <span style="color: white;background-color: #4865A9;">Detailed Summary</span>
                      </div>
                    </div>
                    <div [hidden]="!showMeDetailChild" class="detailview" style="line-height:1.2;margin-left:5%;margin-top:2%;">
                      <div>
                        <span style="text-align: center">
                          <span>
                            <a style="cursor: pointer;color:green" (click)="openDetailsPopUp('D9C61BA2-B','FC_LAC','detail')">FC.20.0.0.1-LAC.20.0.0.1</a>
                            <br/>
                            <a style="cursor: pointer;color:green" (click)="openDetailsPopUp('D9C61BA2-B','LBFR_CR','detail')">LBFR.20.0.0.1-CR.20.0.0.1</a>
                            <br/>
                            <a style="cursor: pointer;color:green" (click)="openDetailsPopUp('D9C61BA2-B','LBFR_LASR','detail')">LBFR.20.0.0.1-LASR.20.0.0.1</a>
                            <br/>
                            <a style="cursor: pointer;color:green" (click)="openDetailsPopUp('D9C61BA2-B','LBFR_LAFR','detail')">LBFR.20.0.0.1-LAFR.20.0.0.1</a>
                            <br/>
                          </span>
                          <!-- <span *ngIf="configDetail.value == 0">
                            <span style="color:black">{{configDetail.key}}</span>
                          </span> -->

                        </span>
                      </div>
                    </div>
                  </td>

                  <!-- <td class="text_bold">{{config.status}}</td>
                  <td class="text_bold">{{config.reqReceivedOn}}</td>
                  <td class="text_bold">{{config.reqProcessesOn}}</td>
                  <td class="text_bold">{{config.comment}}</td> -->

                </tr>

              </tbody>
            </table>
          </div>
          <h3 style="text-align: center;" [hidden]="!errorFlag">Record Not Available</h3>
        </div>
      </div>
    </div>
    <!--    Page Footer Section  -->
    <footer class="text-center">&copy; Tech Mahindra - 2018</footer>
    <!--    Page Footer Section  -->
  </div>
</div>