<div class="container main-pg">

    <div class="row">
  
      <!--    Page Header Section     -->
      <app-header></app-header>
      <div class="pg-bdy">
        <!-- Breadcrumb Section -->
        <ul class="breadcrumb">
          <li><a ui-sref="Home">C3P</a></li>
          <li class="breadcrumbLabel">Admin</li>
          <li class="active">Device Management</li>
        </ul>
        <div class="panel pb-50 form-inline border-top mt-5 lbl-normal cmp-normal">
          <!--    Customer Information     -->
          <br/>
          <section>
            <form #myForm="ngForm" id="devMgmtForm" novalidate>
              <div class="col-md-12 form-group">
              <div class="col-lg-2 mb-15 text-right">
                <h2 class="title second-title pr-3">Device Management</h2>
                
              </div>
              
            </div>
              <div class="col-lg-4 form-group">
                    <label class="col-lg-4 text-right pl-0">
                      Username :</label>
                    <div class="col-lg-8">
                      <input type="text" class="form-control" name="name"
                        [(ngModel)]="deviceMgtUsername" maxlength="20" required>
                    </div>
              </div>
              <div class="col-lg-4 form-group">
                <label class="col-lg-4 text-right pl-0">Password :</label>
                  
                <div class="col-lg-8">
                  <input type="password" class="width100" name="password"
                  [(ngModel)]="deviceMgtPassword" maxlength="12" required/>
                </div>
              </div>
              
          
            <div class="col-lg-2 form-group">
                <button type="submit" class="btn btn-primary ml-10 mr-41 pull-right"
                  (click)="submitDeviceMgtData('devMgmtForm')">Submit</button>
                  
            </div> 
          <div class="row mt-10">
            <!-- <div class="col-lg-12 form-group">
            <button type="submit" class="btn btn-primary ml-5 pull-right"
                ng-click="getallipamData()">Get All Data</button>
            </div> -->
          </div>
          </form>
          </section>
        </div>
    </div>
  </div>  
    <!--    Page Body Section    -->
  
    <!--    Page Footer Section  -->
  <app-footer></app-footer>
  <!--    Page Footer Section  -->
  
  
</div>
  
  
  